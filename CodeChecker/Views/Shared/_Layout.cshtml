<?xml version="1.0" ?>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CodeChecker</title>
    <base href="/">

    <link rel="stylesheet" href="~/static/css/plugins.front.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/static/css/styles.front.css" asp-append-version="true" />
</head>
<body class="@ViewData["Body"]" ng-app="@ViewData["NgApp"]">
<div class="container body-content">
    @Html.Partial("Partials/_Navbar")
    <div class="row">
        <div class="col-md-12">
            @RenderBody()
        </div>
    </div>
    <hr/>
    @Html.Partial("Partials/_Footer")
</div>
<script src="~/static/js/plugins.front.js" asp-append-version="true"></script>
<script src="~/Angular/Front/app-navbar.js" asp-append-version="true"></script>
<script src="~/Angular/Front/Controllers/navbarController.js" asp-append-version="true"></script>
@RenderSection("Scripts", required: false)
</body>
</html>