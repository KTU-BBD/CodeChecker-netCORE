@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<?xml version="1.0" ?>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CodeChecker</title>

    <link rel="stylesheet" href="~/static/css/plugins.front.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/static/css/styles.front.css" asp-append-version="true" />
    @Html.Raw(JavaScriptSnippet.FullScript)
</head>
<body class="@ViewData["Body"]" ng-app="@ViewData["NgApp"]" ng-cloak>
    <div class="container body-content">
        @Html.Partial("Partials/_Navbar")
        <div class="row">
            <div class="col-md-9">
                @RenderBody()
            </div>
            <div class="col-md-3">
                @Html.Partial("Partials/_Sidebar")
            </div>
        </div>
        <hr />
        @Html.Partial("Partials/_Footer")
        </div>
        <script src="~/static/js/plugins.front.js" asp-append-version="true"></script>
        @RenderSection("Scripts", required: false)
</body>
</html>